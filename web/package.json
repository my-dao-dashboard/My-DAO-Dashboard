{
  "private": true,
  "name": "my-dao-dashboard",
  "description": "My DAO Dashboard",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "keywords": [
    "ethereum"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/my-dao-dashboard/My-DAO-Dashboard/My-DAO-Dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/my-dao-dashboard/My-DAO-Dashboard/issues"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts --max_old_space_size=4096 build",
    "test": "react-scripts test --coverage --verbose --detectOpenHandles --forceExit --passWithNoTests",
    "eject": "react-scripts eject",
    "lint": "prettier --check \"src/**/*.ts\"",
    "lint-fix": "prettier --write --check \"src/**/*.ts\"",
    "heroku-postbuild": "yarn install --production=false && yarn build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "3box": "^1.10.7",
    "@datorama/akita": "^4.10.2",
    "antd": "^3.22.0",
    "apollo-boost": "^0.4.4",
    "bignumber.js": "^9.0.0",
    "ethereum-blockies-base64": "^1.0.2",
    "graphql": "^14.5.3",
    "memoizee": "^0.4.14",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "rxjs": "^6.3.3",
    "underscore": "1.9.1",
    "web3": "^1.2.1"
  },
  "devDependencies": {
    "@machinomy/hdwallet-provider": "^1.7.1",
    "@types/fetch-mock": "^7.3.1",
    "@types/jest": "24.0.18",
    "@types/memoizee": "^0.4.2",
    "@types/node": "12.7.2",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-test-renderer": "^16.9.0",
    "@types/underscore": "^1.9.3",
    "@types/web3": "^1.0.19",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "fetch-mock": "^7.3.9",
    "jest-marbles": "^2.5.0",
    "prettier": "^1.18.2",
    "react-scripts": "3.1.1",
    "react-test-renderer": "^16.9.0",
    "typescript": "3.5.3"
  }
}
